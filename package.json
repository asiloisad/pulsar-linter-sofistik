{
  "name": "linter-sofistik",
  "description": "Read and list errors produced by SOFiSTiK",
  "repository": "https://github.com/bacadra/pulsar-linter-sofistik",
  "keywords": [
    "linter",
    "sofistik"
  ],
  "version": "0.0.4",
  "engines": {
    "atom": ">=1.100.0 <2.0.0"
  },
  "main": "./lib/main",
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeIndie"
      }
    }
  },
  "activationHooks": [
    "language-sofistik:grammar-used"
  ],
  "linter-package": true
}
