{
  "name": "linter-sofistik",
  "description": "Display SOFiSTiK compilation errors in Pulsar",
  "repository": "https://github.com/asiloisad/pulsar-linter-sofistik",
  "keywords": [
    "linter",
    "sofistik"
  ],
  "version": "0.2.1",
  "engines": {
    "atom": ">=1.100.0 <2.0.0"
  },
  "activationHooks": [
    "language-sofistik:grammar-used"
  ],
  "main": "./lib/main",
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeIndie"
      }
    },
    "sofistik.keywords": {
      "versions": {
        "2.0.0": "consumeKeywordsService"
      }
    }
  }
}
